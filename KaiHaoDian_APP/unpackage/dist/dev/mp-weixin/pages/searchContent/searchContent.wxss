.status_bar {
  height: 25px;
  width: 100%;
}
.top_view {
  height: 25px;
  width: 100%;
  position: fixed;
  top: 0;
}
.content {
  width: 750rpx;
}
.top_tab {
  width: 750rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  padding: 0 30rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-justify-content: space-around;
      -ms-flex-pack: distribute;
          justify-content: space-around;
  -webkit-box-align: center;
  -webkit-align-items: center;
      -ms-flex-align: center;
          align-items: center;
  height: 94rpx;
}
.lineCss {
  width: 2rpx;
  height: 44rpx;
  background: #999;
}
.tab_child {
  width: 140rpx;
  line-height: 88rpx;
  text-align: center;
  color: 666;
}
.top_tab {
  background: #f5f5f5;
}
.top_tab .tab_child_hover {
    background: #eee9e9;
    border-bottom: 6rpx solid #3285ff;
}
.userList {
  width: 750rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  padding: 0 30rpx 30rpx;
}
.userList .user_cell {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-pack: justify;
    -webkit-justify-content: space-between;
        -ms-flex-pack: justify;
            justify-content: space-between;
    -webkit-box-align: center;
    -webkit-align-items: center;
        -ms-flex-align: center;
            align-items: center;
    margin-top: 30rpx;
    background: #f5f5f5;
}
.userList .user_cell .userCell_L,
    .userList .user_cell .userCell_L image {
      width: 100rpx;
      height: 100rpx;
      font-size: 0;
      border-radius: 50%;
      background: #eee9e9;
}
.userList .user_cell .userCell_C {
      margin-left: 20rpx;
      width: 470rpx;
      line-height: 50rpx;
}
.userList .user_cell .userCell_C .userCell_C_title {
        max-width: 470rpx;
        color: #666;
        font-size: 28rpx;
}
.userList .user_cell .userCell_C .userCell_C_company {
        font-size: 24rpx;
        color: #666;
}
.userList .user_cell .userCell_C .userCell_C_company text {
          display: inline-block;
          vertical-align: middle;
}
.userList .user_cell .userCell_C .userCell_C_company .user_company {
          max-width: 400rpx;
          margin-right: 10rpx;
          overflow: hidden;
          -o-text-overflow: ellipsis;
             text-overflow: ellipsis;
          white-space: nowrap;
}
.userList .user_cell .userCell_C .userCell_C_company .user_company_rz {
          color: #ff8100;
}
.userList .user_cell .userCell_R {
      width: 110rpx;
      font-size: 20rpx;
      color: #666;
      text-align: center;
      line-height: 50rpx;
}
.userList .user_cell .userCell_R view {
        width: 100rpx;
        border-radius: 8rpx;
        background: #EEE9E9;
}

