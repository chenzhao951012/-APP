/* 文章场景相关 */
.status_bar {
  height: 25px;
  width: 100%;
}
.top_view {
  height: 25px;
  width: 100%;
  position: fixed;
  top: 0;
}
.content {
  width: 750rpx;
}
.top_tab {
  width: 750rpx;
  height: 78rpx;
  background: #fff;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 99;
  border-bottom: 2rpx solid #eee;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
      -ms-flex-align: center;
          align-items: center;
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
      -ms-flex-pack: start;
          justify-content: flex-start;
  padding: 0 30rpx;
}
.top_tab ._tab {
    width: 68rpx;
    margin-right: 40rpx;
}
.top_tab ._tab > ._top {
      line-height: 50rpx;
      font-size: 30rpx;
      color: #333;
      letter-spacing: 2rpx;
      text-align: center;
}
.top_tab ._tab > view:last-child {
      width: 68rpx;
      height: 8rpx;
      background: transparent;
      border-radius: 10rpx;
}
.top_tab ._tab > ._topColor {
      -webkit-transition: .5s;
      -o-transition: .5s;
      transition: .5s;
      color: #3285ff;
}
.top_tab ._tab > ._lineCss {
      -webkit-transition: .5s;
      -o-transition: .5s;
      transition: .5s;
      background: #3285ff !important;
}
/* 开店知识 */
